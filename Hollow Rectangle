#include<bits/stdc++.h>
using namespace std;


int main(){
    
    int row = 3;
    int col = 5;
    
    
    for(int  i = 0 ; i< row ; i++){
        if(i == 0 || i == 2){
            for(int  j = 0 ; j < col ; j++){
                cout << "*";
            }
            
        }
        else{
            cout << "*";
            for(int i = 0; i < 3; i++)
            {
                cout << " ";
            }
            cout << "*";
            }
            cout << endl;
        }
 return 0;       
}   
        
        
